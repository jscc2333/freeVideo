    <servlet-name>HistoryLoad</servlet-name>
    <servlet-class>com.servlet.HistoryLoad</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>VideoLoad</servlet-name>
    <servlet-class>com.servlet.VideoLoad</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>VideoView</servlet-name>
    <servlet-class>com.servlet.VideoView</servlet-class>
  </servlet>
  <servlet>
