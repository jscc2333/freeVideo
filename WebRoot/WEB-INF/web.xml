  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CommentRefresh</servlet-name>
    <url-pattern>/servlet/CommentRefresh</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>VideoSearch</servlet-name>
    <url-pattern>/servlet/VideoSearch</url-pattern>
  </servlet-mapping>
</web-app>