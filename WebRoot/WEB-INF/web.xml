    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CommentRefresh</servlet-name>
    <servlet-class>com.servlet.CommentRefresh</servlet-class>
  </servlet>
   <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>VideoSearch</servlet-name>
    <servlet-class>com.servlet.VideoSearch</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/servlet/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HistoryLoad</servlet-name>
    <url-pattern>/servlet/HistoryLoad</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VideoLoad</servlet-name>
    <url-pattern>/servlet/VideoLoad</url-pattern>
